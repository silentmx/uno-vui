{
  "name": "@silentmx/monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.28.0",
  "license": "MIT",
  "scripts": {
    "dev": "nr gen && nr stub",
    "doc": "nr docs:dev",
    "docs:dev": "pnpm --filter @silentmx/v-ui-docs run dev",
    "build": "nr gen && pnpm -r --parallel --filter=./packages/* run build",
    "stub": "pnpm -r --parallel --filter=./packages/* run stub",
    "gen": "nr gen-types && nr gen-volar",
    "gen-types": "na tsx ./scripts/gen-types.ts",
    "gen-volar": "na tsx ./scripts/gen-volar.ts",
    "release": "bumpp package.json packages/**/package.json",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.6",
    "@unocss/reset": "^0.50.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.13.0",
    "bumpp": "^9.0.0",
    "camelcase": "^7.0.1",
    "fs-extra": "^11.1.0",
    "globby": "^13.1.3",
    "jsdom": "^21.1.0",
    "pnpm": "^7.29.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vitest": "^0.29.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
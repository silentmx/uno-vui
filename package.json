{
  "name": "@silentmx/v-ui",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "description": "vue3 based component library built on top of Unocss and Vueuse",
  "author": {
    "name": "silentmx",
    "email": "silentmx@outlook.com",
    "url": "https://github.com/silentmx"
  },
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/silentmx/v-ui",
    "directory": "src"
  },
  "bugs": {
    "email": "silentmx@outlook.com",
    "url": "https://github.com/silentmx/v-ui/issues"
  },
  "keywords": [
    "component library",
    "vue3",
    "unocss",
    "vueuse"
  ],
  "license": "MIT",
  "main": "./dist/v-ui.umd.cjs",
  "module": "./dist/v-ui.js",
  "exports": {
    ".": {
      "import": "./dist/v-ui.js",
      "require": "./dist/v-ui.umd.cjs"
    }
  },
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "types": "./dist/types",
  "scripts": {
    "dev": "pnpm gen-types && pnpm gen-volar && vue-tsc --declaration --emitDeclarationOnly --watch && vite build --watch",
    "build": "pnpm gen-types && pnpm gen-volar && vite build && vue-tsc --declaration --emitDeclarationOnly",
    "gen-volar": "na tsx ./scripts/gen-volar.ts",
    "gen-types": "na tsx ./scripts/gen-types.ts",
    "docs:dev": "pnpm --filter @silentmx/v-ui-docs dev",
    "docs:build": "pnpm --filter @silentmx/v-ui-docs build",
    "docs:preview": "pnpm --filter @silentmx/v-ui-docs preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "update:pkg": "pnpm update --latest && pnpm --filter @silentmx/v-ui-docs update --latest"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@floating-ui/dom": "^1.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.0",
    "@unocss/core": "^0.49.7",
    "@unocss/reset": "^0.49.7",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.13.0",
    "camelcase": "^7.0.1",
    "defu": "^6.1.2",
    "fs-extra": "^11.1.0",
    "globby": "^13.1.3",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^3.12.3",
    "typescript": "~4.9.5",
    "unocss": "^0.49.7",
    "unplugin-auto-import": "^0.14.3",
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-define-options": "^1.2.2",
    "unplugin-vue-macros": "^1.8.2",
    "vite": "^4.1.2",
    "vitest": "^0.28.5",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.4"
  },
  "peerDependencies": {
    "@unocss/reset": "*",
    "@vueuse/core": "*",
    "unocss": "*",
    "vue": "^3.*"
  }
}
{
  "name": "monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run vui:dev\" \"pnpm run docs:dev\"",
    "vui:dev": "pnpm --filter uno-vui run dev",
    "docs:dev": "pnpm --filter @uno-vui/docs run dev",
    "test": "vitest"
  },
  "devDependencies": {
    "@floating-ui/vue": "^0.2.1",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.15.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.13.0",
    "concurrently": "^7.6.0",
    "fs-extra": "^11.1.0",
    "jsdom": "^21.1.0",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.9.5",
    "uno-vui": "workspace:*",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vitepress": "1.0.0-alpha.52",
    "vitest": "^0.29.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
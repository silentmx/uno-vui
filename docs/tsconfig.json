{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "exclude": [
    "**/dist/**",
    "**/cache/**",
  ],
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.vue",
    ".vitepress/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "ignoreDeprecations": "5.0",
    "types": [
      "vite/client",
      "uno-vui/volar",
    ],
    "paths": {
      "uno-vui": [
        "../packages/uno-vui/lib"
      ]
    }
  },
  "references": [
    {
      "path": "../tsconfig.node.json"
    }
  ]
}
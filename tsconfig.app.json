{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "exclude": [
    "**/dist/**",
    "**/node_modules/**",
    "**/test/dts/**",
    "**/cache/**",
    "**/__tests__/*",
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "vite/client",
      "vitest/globals"
    ],
    "paths": {
      "uno-vui": [
        "./packages/uno-vui/lib"
      ],
      "uno-vui/*": [
        "./packages/uno-vui/lib/*"
      ]
    }
  }
}
{
  "name": "@silentmx/monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.27.1",
  "license": "MIT",
  "scripts": {
    "dev": "nr gen && nr stub",
    "docs:dev": "pnpm --filter @silentmx/v-ui-docs run dev",
    "stub": "pnpm -r --parallel --filter=./packages/* run stub",
    "gen": "pnpm gen-types && pnpm gen-volar",
    "gen-types": "na tsx ./scripts/gen-types.ts",
    "gen-volar": "na tsx ./scripts/gen-volar.ts",
    "release": "bumpp package.json packages/**/package.json",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@silentmx/v-ui": "workspace:^0.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "bumpp": "^9.0.0",
    "camelcase": "^7.0.1",
    "fs-extra": "^11.1.0",
    "jsdom": "^21.1.0",
    "pnpm": "^7.27.1",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "unocss": "^0.50.1",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vitest": "^0.28.5",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.7"
  }
}
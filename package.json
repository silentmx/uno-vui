{
  "name": "monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm vui:build && concurrently pnpm:vui:dev pnpm:docs:dev",
    "build": "pnpm vui:build && pnpm docs:build",
    "vui:dev": "pnpm --filter uno-vui run dev",
    "vui:build": "pnpm --filter uno-vui run build",
    "docs:dev": "pnpm --filter @uno-vui/docs run dev",
    "docs:build": "pnpm --filter @uno-vui/docs run build",
    "test": "vitest"
  },
  "devDependencies": {
    "@floating-ui/vue": "^0.2.1",
    "@iconify/json": "^2.2.48",
    "@iconify/tools": "^2.2.6",
    "@types/fs-extra": "^11.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.13.0",
    "concurrently": "^8.0.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^21.1.1",
    "markdown-it": "^13.0.1",
    "tsx": "^3.12.6",
    "typescript": "~5.0.4",
    "uno-vui": "workspace:*",
    "unocss": "^0.51.4",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vitepress": "1.0.0-alpha.65",
    "vitest": "^0.30.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
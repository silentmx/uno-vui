{
  "name": "@silentmx/v-ui",
  "type": "module",
  "version": "0.0.1",
  "description": "vue3 based component library built on top of Unocss and Vueuse",
  "author": {
    "email": "silentmx@outlook.com",
    "name": "silentmx",
    "url": "https://github.com/silentmx"
  },
  "license": "MIT",
  "homepage": "",
  "repository": {
    "type": "git",
    "directory": "packages/v-ui",
    "url": "git+https://github.com/silentmx/v-ui.git"
  },
  "bugs": {
    "email": "silentmx@outlook.com",
    "url": "https://github.com/silentmx/v-ui/issues"
  },
  "keywords": [
    "vue3",
    "components",
    "unocss",
    "vueuse"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/v-ui.umd.cjs",
      "import": "./dist/v-ui.js"
    }
  },
  "main": "./dist/v-ui.umd.cjs",
  "module": "./dist/v-ui.js",
  "types": "./dist/types",
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck && vite build",
    "stub": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck && vite build --watch"
  },
  "devDependencies": {
    "@unocss/reset": "^0.50.3",
    "@vueuse/core": "^9.13.0",
    "unocss": "^0.50.3",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "@unocss/reset": "*",
    "@vueuse/core": "*",
    "unocss": "*",
    "vue": "^3.*"
  },
  "publishConfig": {
    "access": "public"
  }
}